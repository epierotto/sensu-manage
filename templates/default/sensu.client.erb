{
  "rabbitmq": {
    "ssl": {
      "cert_chain_file": "<%= @ssl_cert_chain_file %>",
      "private_key_file": "<%= @ssl_private_key_file %>"
    },
    "host": "<%= node['sensu-manage']['rabbitmq']['host'] %>",
    "port": <%= node['sensu-manage']['rabbitmq']['port'] %>,
    "vhost": "<%= node['sensu-manage']['rabbitmq']['vhost'] %>",
    "user": "<%= node['sensu-manage']['rabbitmq']['user'] %>",
    "password": "<%= node['sensu-manage']['rabbitmq']['password'] %>"
  },
  
  "client": {
    "name": "<%= node[:fqdn] %>",
    "address": "<%= node[:ipaddress] %>",
    "subscriptions": <%= @subscriptions %>
  }
}
